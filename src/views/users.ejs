<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Email</th>
    <th>Password</th>
  </tr>
  <% users.forEach(user => { %>
  <tr>
    <td><%= user.firstname %></th>
    <td><%= user.lastname %></th>
    <td><%= user.email %></th>
    <td><%= user.password %></th>
  </tr>
  <% }) %>
</table>

